        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "husky": "^7.0.0",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "lint-staged": "^12.1.3",
        "postcss": "^8.4.7",
        "postcss-px-to-viewport": "^1.1.1",
